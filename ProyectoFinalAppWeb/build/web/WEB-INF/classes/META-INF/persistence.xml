  <persistence-unit name="objetosNegocioPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>objetosnegocio.Admor</class>
    <class>objetosnegocio.Anclado</class>
    <class>objetosnegocio.Comentario</class>
    <class>objetosnegocio.Comun</class>
    <class>objetosnegocio.Normal</class>
    <class>objetosnegocio.Post</class>
    <class>objetosnegocio.Usuario</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/proyecto_appweb?zeroDateTimeBehavior=CONVERT_TO_NULL"/>
